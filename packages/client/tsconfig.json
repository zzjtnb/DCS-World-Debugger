{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "types/**/*",
    "types/**/*.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "noImplicitAny": false,
    // "strictNullChecks":false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "typeRoots": [
      "node_modules/@types",
      "types"
    ],
    "types": [
      "node",
      "vite/client"

    ]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
