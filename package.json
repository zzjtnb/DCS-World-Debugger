{
  "name": "server",
  "version": "0.3.0",
  "description": "DCS World ",
  "author": "zzjtnb",
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --trace-warnings app.js ",
    "dev": "cross-env NODE_ENV=development  nodemon app.js",
    "prod": "cross-env NODE_ENV=production nodemon app.js",
    "build": "npm run tailwind:css && nodemon app.js",
    "tailwind:css": "postcss public/css/tailwind.css -o public/css/style.css",
    "dbsync": "cross-env NODE_ENV=development  node models/dbsync.js",
    "automate": "cross-env NODE_ENV=development  node config/db/automate.js"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      "logs/*",
      "scripts"
    ]
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "dayjs": "^1.10.4",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "highlight.js": "^10.7.1",
    "log4js": "^6.3.0",
    "mariadb": "^2.5.3",
    "nodemon": "^2.0.7",
    "oicq": "^1.14.2",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "sequelize": "^6.6.1",
    "sequelize-automate": "^1.2.2",
    "socket.io": "^2.4.1",
    "tailwindcss": "^2.0.4",
    "xss": "^1.0.8"
  }
}
