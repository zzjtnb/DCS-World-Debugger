{
  "name": "dcs_world_lua_debuger",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start:node": "node --inspect app.js",
    "start:nmon": "nodemon --trace-warnings app.js",
    "dev": "cross-env NODE_ENV=development nodemon app.js",
    "prod": "cross-env NODE_ENV=production nodemon app.js",
    "dbsync": "cross-env NODE_ENV=development node models/dbsync.js",
    "automate": "cross-env NODE_ENV=development node config/db/automate.js",
    "dev:inspect": "cross-env NODE_ENV=development nodemon --inspect app.js",
    "build": "npm run tailwind:css & cross-env NODE_ENV=development nodemon app.js",
    "tailwind:css": "postcss public/stylesheets/tailwind.css -o public/stylesheets/style.css"
  },
  "_moduleAliases": {
    "@": "."
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "chalk": "^5.0.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "log4js": "^6.3.0",
    "luaparse": "^0.3.1",
    "module-alias": "^2.2.2",
    "node-stream-zip": "^1.15.0",
    "nodemon": "^2.0.14",
    "socket.io": "^4.3.1",
    "stacktrace-js": "^2.0.2",
    "tailwindcss": "^2.2.17",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1"
  }
}
